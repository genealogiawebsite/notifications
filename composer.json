{
    "name": "genealogiawebsite/notifications",
    "description": "Push notifications dependency for LaravelEnso",
    "keywords": [
        "genealogiawebsite",
        "notifications",
        "push-notifications",
        "laravel-notifications"
    ],
    "homepage": "https://github.com/genealogiawebsite/notifications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://genealogiawebsite.com",
            "role": "Developer"
        }
    ],
    "require": {
        "genealogiawebsite/core": "^5.0|^6.0",
        "genealogiawebsite/migrator": "^2.0",
        "php": ">=7.4.0",
        "pusher/pusher-php-server": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Notifications\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Notifications\\AppServiceProvider",
                "LaravelEnso\\Notifications\\BroadcastServiceProvider"
            ],
            "aliases": {}
        }
    }
}
